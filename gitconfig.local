  name = Kevin Plotner
  email = secoast@gmail.com

[alias]
  up = !git fetch origin && git rebase origin/master
  l = log --graph --decorate --oneline

  merge-branch = !git checkout master && git merge @{-1}
  rename-branch = !sh -c 'old=$(git current-branch) && git branch -m $old $1 && git push origin --set-upstream $1 && git push origin --delete $old' - st = status

